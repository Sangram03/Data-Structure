// C++ CODE

    int findWinner(int n, int A[]){
        // code here
        int x=0;
        for(int i=0;i<n;i++)x^=A[i];
        if(x==0 || n%2==0)return 1;
        return 2;
    }

// JAVA CODE

    static int findWinner(int n, int A[]){
        // code here
        int x=0;
        for(int i=0;i<n;i++)x^=A[i];
        if(x==0 || n%2==0)return 1;
        return 2;
    }
